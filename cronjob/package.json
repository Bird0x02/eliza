{
    "name": "cronjob",
    "version": "0.0.1",
    "main": "src/index.ts",
    "type": "module",
    "scripts": {
        "start": "node --loader ts-node/esm src/index.ts",
        "dev": "node --loader ts-node/esm src/index.ts",
        "check-types": "tsc --noEmit",
        "test": "jest"
    },
    "nodemonConfig": {
        "watch": [
            "src",
            "../core/dist"
        ],
        "ext": "ts,json",
        "exec": "node --enable-source-maps --loader ts-node/esm src/index.ts"
    },
    "dependencies": {
        "@elizaos/adapter-bullmq": "workspace:*",
        "@elizaos/adapter-postgres": "workspace:*",
        "@elizaos/adapter-redis": "workspace:*",
        "@elizaos/core": "workspace:*",
        "@types/node": "^22.13.1",
        "cheerio": "^1.0.0",
        "google-auth-library": "^9.15.1",
        "googleapis": "^144.0.0",
        "node-cache": "5.1.2",
        "puppeteer": "^24.1.1",
        "puppeteer-extra": "3.3.6",
        "puppeteer-extra-plugin-stealth": "^2.11.2",
        "readline": "1.3.0",
        "ws": "8.18.0",
        "yargs": "17.7.2"
    },
    "devDependencies": {
        "@types/jest": "^29.5.14",
        "jest": "^29.7.0",
        "ts-jest": "^29.2.5",
        "ts-node": "10.9.2",
        "tsup": "8.3.5"
    }
}
